                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="17"
                    app:layout_constraintLeft_toRightOf="@id/textViewLabel_fragment_ltebasestationcelldetailinfo_pci"
                    app:layout_constraintTop_toBottomOf="@id/textViewLabel_fragment_ltebasestationcelldetailinfo_tac"/>

                <TextView
                    android:id="@+id/textViewLabel_fragment_ltebasestationcelldetailinfo_lteEarFcn"
