                    android:id="@+id/textView_fragment_ltebasestationcelldetailinfo_lteEarFcn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="18"
                    app:layout_constraintLeft_toRightOf="@id/textViewLabel_fragment_ltebasestationcelldetailinfo_lteEarFcn"
