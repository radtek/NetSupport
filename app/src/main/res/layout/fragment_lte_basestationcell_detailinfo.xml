                    app:layout_constraintTop_toBottomOf="@id/textViewLabel_fragment_ltebasestationcelldetailinfo_name"/>

                <TextView
                    android:id="@+id/textViewLabel_fragment_ltebasestationcelldetailinfo_lng"
