                    app:layout_constraintTop_toBottomOf="@id/textViewLabel_fragment_ltebasestationcelldetailinfo_city"/>

                <TextView
                    android:id="@+id/textView_fragment_ltebasestationcelldetailinfo_lng"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="4"
                    app:layout_constraintLeft_toRightOf="@id/textViewLabel_fragment_ltebasestationcelldetailinfo_lng"
                    app:layout_constraintTop_toBottomOf="@id/textViewLabel_fragment_ltebasestationcelldetailinfo_city"/>

                <TextView
                    android:id="@+id/textViewLabel_fragment_ltebasestationcelldetailinfo_lat"
