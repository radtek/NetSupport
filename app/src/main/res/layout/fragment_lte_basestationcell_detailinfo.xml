                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="eci："
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>

                <TextView
                    android:id="@+id/textView_fragment_ltebasestationcelldetailinfo_eci"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1"
                    app:layout_constraintLeft_toRightOf="@id/textViewLabel_fragment_ltebasestationcelldetailinfo_eci"
                    app:layout_constraintTop_toTopOf="parent"/>

                <TextView
                    android:id="@+id/textViewLabel_fragment_ltebasestationcelldetailinfo_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="小区名："
