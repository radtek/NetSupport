                    android:layout_height="wrap_content"
                    android:text="5"
                    app:layout_constraintLeft_toRightOf="@id/textViewLabel_fragment_ltebasestationcelldetailinfo_lat"
                    app:layout_constraintTop_toBottomOf="@id/textViewLabel_fragment_ltebasestationcelldetailinfo_lng"/>

                <TextView
