                    android:text="室内外："
                    app:layout_constraintTop_toBottomOf="@id/textViewLabel_fragment_ltebasestationcelldetailinfo_altitude"/>

                <TextView
                    android:id="@+id/textView_fragment_ltebasestationcelldetailinfo_indoorOrOutdoor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="8"
                    app:layout_constraintLeft_toRightOf="@id/textViewLabel_fragment_ltebasestationcelldetailinfo_indoorOrOutdoor"
                    app:layout_constraintTop_toBottomOf="@id/textViewLabel_fragment_ltebasestationcelldetailinfo_altitude"/>

                <TextView
                    android:id="@+id/textViewLabel_fragment_ltebasestationcelldetailinfo_coverageType"
